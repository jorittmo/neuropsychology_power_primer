@article{crawford_comparing_1998,
	title = {Comparing an {Individual}'s {Test} {Score} {Against} {Norms} {Derived} from {Small} {Samples}},
	volume = {12},
	issn = {1385-4046, 1744-4144},
	url = {https://www.tandfonline.com/doi/full/10.1076/clin.12.4.482.7241},
	doi = {10.1076/clin.12.4.482.7241},
	abstract = {The standard method for comparing an individual’s test score with a normative sample involves converting the score to a z score and evaluating it using a table of the area under the normal curve. When the normative sample is small, a more appropriate method is to treat the individual as a sample of N = 1 and use a modiﬁed t test described by Sokal and Rohlf (1995). The use of this t test is illustrated with examples and its results compared to those from the standard procedure. It is suggested that the t test be used when the N of the normative sample is less than 50. Finally, a computer program that implements the modiﬁed t-test procedure is described. This program can be downloaded from the ﬁrst author’s website.},
	language = {en},
	number = {4},
	urldate = {2020-02-04},
	journal = {The Clinical Neuropsychologist},
	author = {Crawford, John and Howell, D},
	month = nov,
